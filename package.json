{
  "name": "zhique-editor",
  "version": "1.2.2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhique-design/zhique-editor.git"
  },
  "bugs": {
    "url": "https://github.com/zhique-design/zhique-editor/issues"
  },
  "homepage": "https://zhique-design.github.io/zhique-editor",
  "author": "xuzhao <xuzhao@zhique.design>",
  "license": "MIT",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start:demo": "webpack-dev-server --config webpack.config.dev.js",
    "build:demo": "cross-env NODE_ENV=production webpack --mode production --config webpack.config.dev.js",
    "clean:dist": "rimraf ./dist",
    "copy:files": "copyfiles -u 1 src/**/*.less src/**/*.js dist/",
    "build:ts": "yarn run clean:dist && tsc -p tsconfig.production.json && yarn run copy:files",
    "deploy": "yarn run build:demo && gh-pages -d demo/dist"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/react": "^16.14.32",
    "@types/react-dom": "^16.9.16",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "gh-pages": "^4.0.0",
    "github-markdown-css": "^5.1.0",
    "html-webpack-plugin": "^5.5.0",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "mini-css-extract-plugin": "^2.6.1",
    "rimraf": "^3.0.2",
    "source-map-loader": "^4.0.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpackbar": "^5.0.2"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "codemirror": "^5.65.9",
    "mobx": "^6.7.0",
    "mobx-react": "^7.6.0",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^5.0.3",
    "react-syntax-highlighter": "^15.5.0",
    "remark-gfm": "^1.0.0"
  }
}
